#include <iostream>
using namespace std;

int main(){
	double x, y, max, min;
	max = -10000;
	min = 10000;
	cout<<"Enter x: ";
	cin>>x;
	cout<<"Enter y: ";
	cin>>y;
	
	if (x > y){
		max = x;
		min = y;
	}else if (x < y){
		max = y;
		min = x;
	}
	if (x < y or y < x){
		cout<<"Max = "<<max;
		cout<<"\nMin = "<<min;
	}else{
		cout<<"x == y";
	}
	return 0;
}
